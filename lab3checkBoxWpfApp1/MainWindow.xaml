<Window x:Class="lab3checkBoxWpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab3checkBoxWpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel x:Name="stackPanel">
        <CheckBox x:Name="checkBox1" IsThreeState="True" IsChecked="False" Height="50" Content="Not marked" Checked="checkBox1_Checked"/>
        <CheckBox x:Name="checkBox2" IsThreeState="True" IsChecked="True" Height="50" Content="Marked"/>
        <CheckBox x:Name="checkBox3" IsThreeState="True" IsChecked="{x:Null}" Height="50" Content="Unknown"/>
    </StackPanel>
</Window>
